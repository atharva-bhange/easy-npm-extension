{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["dom", "es2019"],
    "module": "ESNext",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "strict": false,
    "target": "es2018",
    "baseUrl": "src",
    "typeRoots": ["./src/@types", "./node_modules/@types"]
  }
}
